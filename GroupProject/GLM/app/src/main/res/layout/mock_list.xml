<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_back_mocklist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="323dp"
        android:layout_marginBottom="93dp"
        android:text="Back"
        app:layout_constraintBottom_toTopOf="@+id/btn_item_mocklist"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_name_mocklist"
        android:layout_width="199dp"
        android:layout_height="56dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="71dp"
        android:text=" Mock List #"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_search_item_mocklist"
        android:layout_width="176dp"
        android:layout_height="47dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="300dp"
        android:layout_marginEnd="16dp"
        android:text="Search Item"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_search_type_mocklist"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_check_mocklist"
        app:layout_constraintVertical_bias="0.651" />

    <Button
        android:id="@+id/btn_search_type_mocklist"
        android:layout_width="178dp"
        android:layout_height="51dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="68dp"
        android:text="Search Type"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_search_item_mocklist" />

    <Button
        android:id="@+id/btn_return_to_Homepage_mocklist"
        android:layout_width="290dp"
        android:layout_height="46dp"
        android:layout_marginStart="118dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="67dp"
        android:text="Return to Homepage"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_search_item_mocklist" />

    <Button
        android:id="@+id/btn_item_mocklist"
        android:layout_width="114dp"
        android:layout_height="44dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="29dp"
        android:layout_marginBottom="511dp"
        android:text="Item#"
        app:layout_constraintBottom_toTopOf="@+id/btn_search_item_mocklist"
        app:layout_constraintStart_toEndOf="@+id/btn_check_mocklist"
        app:layout_constraintTop_toBottomOf="@+id/tv_name_mocklist"
        app:layout_constraintVertical_bias="0.04" />

    <EditText
        android:id="@+id/et_quantity_mocklist"
        android:layout_width="41dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="text"
        android:text="1"
        tools:layout_editor_absoluteX="289dp"
        tools:layout_editor_absoluteY="136dp" />

    <Button
        android:id="@+id/btn_check_mocklist"
        android:layout_width="63dp"
        android:layout_height="47dp"
        android:text="C"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="142dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="139dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="69dp" />

    <TableLayout
        android:layout_width="409dp"
        android:layout_height="489dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="104dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
